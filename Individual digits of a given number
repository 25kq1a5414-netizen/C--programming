// function definition 
void print digits(int num){
int rev=0, digit;
// reverse the number first ( to print in correct order)
while(num>0){
digit=num%10;
rev=rev*10+digit;
num/=10;
}
// print digits one by one
while ( rev>0){
digit= rev%10;
printf("%d\n", digit);
rev/=10;
}
}
