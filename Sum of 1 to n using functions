#include<stdio.h>
int sumN(int n);
int main()
{
 int n, result;
 printf(" enter the value of n:");
 scanf("%d",&n);
 result=sumN(n);//function call
 printf("sum of numbers from 1to %d is:%d\n",n,result);
 return 0;
}
  // function definition 
     int sumN( int n){
     int sum=0;
     for(int i=1; i<=n; i++){
      sum+=i;
}
  return sum;// return the result 
}
